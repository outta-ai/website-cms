{
	"name": "outta-website-cms",
	"version": "1.0.0",
	"description": "OUTTA main website CMS, provided with Payload CMS",
	"license": "MIT",
	"main": "dist/server.js",
	"scripts": {
		"build": "pnpm generate:types && pnpm copyfiles && pnpm build:payload && pnpm build:server",
		"build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build",
		"build:server": "tsc",
		"copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
		"dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon",
		"generate:graphQLSchema": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:graphQLSchema",
		"generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types && pnpm ts-to-zod src/payload-types.ts src/payload-zod.ts",
		"serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.609.0",
		"@aws-sdk/lib-storage": "^3.609.0",
		"@aws-sdk/s3-request-presigner": "^3.609.0",
		"@payloadcms/bundler-vite": "^0.1.7",
		"@payloadcms/bundler-webpack": "^1.0.6",
		"@payloadcms/db-mongodb": "^1.5.2",
		"@payloadcms/richtext-slate": "^1.5.2",
		"@tailwindcss/typography": "^0.5.13",
		"@tiptap/extension-image": "^2.4.0",
		"@tiptap/pm": "^2.4.0",
		"@tiptap/react": "^2.4.0",
		"@tiptap/starter-kit": "^2.4.0",
		"aws-crt": "^1.21.3",
		"classnames": "^2.5.1",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"cross-env": "^7.0.3",
		"dotenv": "^16.4.5",
		"express": "^4.19.2",
		"jose": "^5.6.3",
		"node-cache": "^5.1.2",
		"path": "^0.12.7",
		"payload": "^2.23.1",
		"prosemirror-model": "^1.21.3",
		"prosemirror-state": "^1.4.3",
		"prosemirror-view": "^1.33.8",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-icons": "^5.2.1",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"@types/cookie-parser": "^1.4.7",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"autoprefixer": "^10.4.19",
		"copyfiles": "^2.4.1",
		"nodemon": "^3.1.4",
		"postcss": "^8.4.39",
		"tailwindcss": "^3.4.4",
		"ts-node": "^10.9.2",
		"ts-to-zod": "^3.8.7",
		"typescript": "^5.4.5"
	}
}